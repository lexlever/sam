
<%- include ('partial/header') -%>
<div class="row products">
    <% products.forEach((p) => { %> 
        <div class="col-xs-12 col-md-4">
            <a href="/products/product/<%= p.category %>/<%= p.slog %>" class="pa">
                <img src="<%= p.imgSrc %>" alt="" class="ui small image p">
                <h3><%= p.title %> </h3>
                <h4>$<%= parseFloat(p.price).toFixed(2) %> </h4>
                <a href="/products/product/<%= p.category %>/<%= p.slug %>" class="vd">View details</a>
            </a>
        </div>
        <% }) %> 
</div>
<%- include ('partial/footer') -%>